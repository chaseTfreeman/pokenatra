<form action="/pokemons/<%= @pokemon.id %>" method="post" >
  <input type="hidden" name="_method" value="put">
  <label>Artist Name</label>
  <input type="text" name="pokemon[name]" value="<%= @pokemon.name %>">
  <label>Photo_url</label>
  <input type="text" name="pokemon[photo_url]" value="<%= @pokemon.photo_url %>">
  <label>Nationality</label>
  <input type="text" name="pokemon[nationality]" value="<%= @pokemon.nationality %>">
  <input type="submit" value="Update pokemon">
</form>

<h2> Delete Artist</h2>
<form action="/pokemons/<%= @pokemon.id %>" method="post" >
  <input type="hidden" name="_method" value="delete">
  <input type="submit" value="Delete Pokemon">
</form>
